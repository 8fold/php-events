{
  "name": "8fold/php-events",
  "description": "A library that facilitates tracking and displaying events",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@8fold.pro"
    }
  ],
  "require": {
    "php": ">=7.0",
    "8fold/php-shoop-extras": "0.1.*",
    "8fold/php-markup": "0.1.*",
    "nesbot/carbon": "2.36.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.1.*",
    "phpunit/php-code-coverage": "8.0.*"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\Events\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\Events\\Tests\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
